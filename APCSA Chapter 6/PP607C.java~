public class PP607C
{
  public static void main (String[] args)
  {
    for (int row = 1; row <= 10; row++)
    {
      for (int spaces = 1; spaces <= 10-row+1; spaces++)
      {
        System.out.print ("*");
      }
      for (int stars = 1; stars <= row; stars++)
      {
        System.out.print("*");
      }
      System.out.println("");
    }
  }
}